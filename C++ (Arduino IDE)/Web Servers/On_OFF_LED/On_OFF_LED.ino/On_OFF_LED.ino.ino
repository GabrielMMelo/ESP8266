void consultaServidor(){
  WiFiClient client = server.available();
  if (!client) {
    return;
  }
  String leitura = client.readStringUntil('\r');
  Serial.println(leitura);
  client.flush();

  if (leitura.indexOf("liga") != -1){
    digitalWrite(led, HIGH);
    String status = digitalRead(led) ? "Ligado" : "Desligado";
    String buf = createBuf(status);
    client.print(buf);
    client.flush();
  }
  else if (leitura.indexOf("off") != -1){
    digitalWrite(led, LOW);
    String status = digitalRead(led) ? "Ligado" : "Desligado";
    String buf = createBuf(status);
    client.print(buf);
    client.flush();
  }
  else {
    Serial.println("Requisição inválida");
    String status = digitalRead(led) ? "Ligado" : "Desligado"; 
    String buf = createBuf(status);
    client.print(buf);
    client.flush();
    client.stop();
  }
}

void loop(){
  consultaServidor();
}
